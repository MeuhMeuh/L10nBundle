{
    "name": "lafourchette/l10n-bundle",
    "description": "A bundle to manage localization",
    "type": "symfony-bundle",
    "keywords": ["localization", "L10N"],
    "minimum-stability": "dev",
    "license": "LGPL-3.0+",
    "authors": [
        {
            "name": "Cyril Otal",
            "email": "cotal@lafourchette.com"
        },
        {
            "name": "LaFourchette LAB",
            "email": "lab@lafourchette.com"
        }
    ],
    "require": {
        "php": ">=5.3.2",
        "symfony/framework-bundle": "~2.0",
        "symfony/yaml": "*",
        "ml/json-ld": "1.*",
        "twig/extensions": "*",
        "twig/twig": "~1.13",
        "doctrine/cache": "~1.0"
    },
    "require-dev": {
        "phpunit/phpunit": "3.7.*"
    },
    "autoload": {
        "psr-0": { "L10nBundle": "" }
    },
    "target-dir": "L10nBundle"
}
